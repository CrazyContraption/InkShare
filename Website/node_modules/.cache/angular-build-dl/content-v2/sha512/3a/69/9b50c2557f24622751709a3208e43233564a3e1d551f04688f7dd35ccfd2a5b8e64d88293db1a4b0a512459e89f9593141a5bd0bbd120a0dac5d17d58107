        key: "saveCanvas",
        value: function saveCanvas() {
          var _this5 = this;

          if (this.disableDrawing) {
            this.snackBar.open("Sorry, the canvas is busy at this time. Try again in a moment.", "Oops", {
              duration: 4000
            });
          }

          this.disableDrawing = true;
          this.image = this.p.get();
          this.canvas.image = [undefined];
          this.canvas.image.pop();

          for (var row = 0; row < this.canvas.dimensions.height; row++) {
            this.p.copy(this.image, row, 0, this.canvas.dimensions.width, row, row, 0, this.canvas.dimensions.width, row);
            this.canvas.image.push(btoa(new Uint8Array(this.image.pixels).reduce(function (data, byte) {
              return data + String.fromCharCode(byte);
            }, '')));
          }

          this.authService.updateCanvas(this.canvas).subscribe(function (data) {
            _this5.snackBar.open("Success, your canvas was saved successfully.", "Thanks", {
              duration: 3200
            });
          });
          this.disableDrawing = false;
        }
      }, {
        key: "loadCanvas",
        value: function loadCanvas() {
          this.disableDrawing = true;
          var index = 0; //(this.canvas.dimensions.height * this.canvas.dimensions.width * 4) - 1;

          for (var row = 0; row < this.canvas.dimensions.height; row++) {
            var pixels = new Uint8Array(atob(this.canvas.image[row]).split("").map(function (c) {
              return c.charCodeAt(0);
            }));

            for (var col = 0; col < this.canvas.dimensions.width; col++) {
              //console.log(`${index} (${row}, ${col}) - [${pixels[index - 3]}, ${pixels[index - 2]},${pixels[index - 1]}, ${pixels[index - 0]}]`)
              var color = void 0;
              if (pixels[index] != undefined) color = this.p.color(pixels[index + 0], pixels[index + 1], pixels[index + 2], pixels[index + 3]);else color = this.p.color(0, 0, 0, 0);
              index += 4;
              this.p.set(col, row, color);
            }
          }

          this.p.updatePixels();
          this.disableDrawing = false;
        }
