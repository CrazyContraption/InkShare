      }, {
        key: "ngOnInit",
        value: function ngOnInit() {
          var _this9 = this;

          try {
            this.authService.reqresCanvasGetById(localStorage.getItem('canvas')).subscribe(function (data1) {
              _this9.canvas = data1; //implement p5.js functions and socket.io listeners/emitters inside the const s variable 

              var s = function s(p) {
                var canvas;

                p.preload = function () {
                  console.log('Preload Complete');
                };

                p.setup = function () {
                  try {
                    console.log('Canvas Setup');
                    canvas = p.createCanvas(_this9.canvas.dimensions.width, _this9.canvas.dimensions.height);
                    canvas.parent('canvas-div');
                    p.background(_this9.backgroundR, _this9.backgroundG, _this9.backgroundB);
                    _this9.p = p;

                    _this9.loadCanvas();
                  } catch (error) {
                    _this9.disableDrawing = false;
                    console.log(error); //location.reload(); // Temp solution to null canvases
                  }
                }; //called when mouse is being held down and dragged


                p.mouseDragged = function () {
                  if (_this9.permissions == "Visitor" || _this9.disableDrawing) return;
                  var retData = new returnData();
                  retData.x = p.mouseX;
                  if (retData.x > _this9.canvas.dimensions.width || retData.x < 0) return;
                  retData.y = p.mouseY;
                  if (retData.y > _this9.canvas.dimensions.height || retData.y < 0) return;
                  retData.r = _this9.selectedRed;
                  retData.g = _this9.selectedGreen;
                  retData.b = _this9.selectedBlue;
                  retData.backgroundR = _this9.backgroundR;
                  retData.backgroundG = _this9.backgroundG;
                  retData.backgroundB = _this9.backgroundB;
                  retData.room = _this9.room;
                  retData.brush = _this9.selectedBrush;
                  retData.brushSize = _this9.selectedSize;
                  retData.fillShape = _this9.fillShape;
                  retData.outlineWidth = _this9.outlineWidth;
                  retData.outlineRed = _this9.outlineRed;
                  retData.outlineGreen = _this9.outlineGreen;
                  retData.outlineBlue = _this9.outlineBlue;
                  retData.showOutline = _this9.showOutline;
                  retData.eraser = _this9.erase;

                  if (_this9.fillShape) {
                    if (_this9.erase) p.fill(_this9.backgroundR, _this9.backgroundG, _this9.backgroundB);else p.fill(_this9.selectedRed, _this9.selectedGreen, _this9.selectedBlue);
                  } else {
                    p.noFill();
                  }

                  if (_this9.showOutline == true) {
                    p.strokeWeight(_this9.outlineWidth);

                    if (_this9.erase) {
                      p.stroke(_this9.backgroundR, _this9.backgroundG, _this9.backgroundB);
                    } else {
                      p.stroke(_this9.outlineRed, _this9.outlineGreen, _this9.outlineBlue);
                    }
                  } else {
                    p.noStroke();
                  }

                  if (_this9.selectedBrush == 'ellipse') {
                    p.ellipse(p.mouseX, p.mouseY, _this9.selectedSize, _this9.selectedSize);
                  } else if (_this9.selectedBrush == 'rectangle') {
                    p.rect(p.mouseX, p.mouseY, _this9.selectedSize, _this9.selectedSize);
                  } else if (_this9.selectedBrush == 'triangle') {
                    p.triangle(p.mouseX, p.mouseY, p.mouseX + _this9.selectedSize / 2, p.mouseY - _this9.selectedSize, p.mouseX + _this9.selectedSize, p.mouseY);
                  } else if (_this9.selectedBrush == 'diamond') {
                    p.quad(p.mouseX, p.mouseY, p.mouseX + _this9.selectedSize / 2, p.mouseY - _this9.selectedSize, p.mouseX + _this9.selectedSize, p.mouseY, p.mouseX + _this9.selectedSize / 2, p.mouseY + _this9.selectedSize);
                  } //emit mouse data to socket.io server


                  _this9.webSocketService.emit('mouse', retData);

                  _this9.p = p;
                }; //called when the mouse is pressed initially (mouse velocity = 0)


                p.mousePressed = function () {
                  if (_this9.permissions == "Visitor" || _this9.disableDrawing) return;
                  var retData = new returnData();
                  retData.x = p.mouseX;
                  if (retData.x > _this9.canvas.dimensions.width || retData.x < 0) return;
                  retData.y = p.mouseY;
                  if (retData.y > _this9.canvas.dimensions.height || retData.y < 0) return;
                  retData.r = _this9.selectedRed;
                  retData.g = _this9.selectedGreen;
                  retData.b = _this9.selectedBlue;
                  retData.backgroundR = _this9.backgroundR;
                  retData.backgroundG = _this9.backgroundG;
                  retData.backgroundB = _this9.backgroundB;
                  retData.room = _this9.room;
                  retData.brush = _this9.selectedBrush;
                  retData.brushSize = _this9.selectedSize;
                  retData.fillShape = _this9.fillShape;
                  retData.outlineWidth = _this9.outlineWidth;
                  retData.outlineRed = _this9.outlineRed;
                  retData.outlineGreen = _this9.outlineGreen;
                  retData.outlineBlue = _this9.outlineBlue;
                  retData.showOutline = _this9.showOutline;
                  retData.eraser = _this9.erase;

                  if (_this9.fillShape) {
                    if (_this9.erase) {
                      p.fill(_this9.backgroundR, _this9.backgroundG, _this9.backgroundB);
                    } else {
                      p.fill(_this9.selectedRed, _this9.selectedGreen, _this9.selectedBlue);
                    }
                  } else {
                    p.noFill();
                  }

                  if (_this9.showOutline == true) {
                    p.strokeWeight(_this9.outlineWidth);

                    if (_this9.erase) {
                      p.stroke(_this9.backgroundR, _this9.backgroundG, _this9.backgroundB);
                    } else {
                      p.stroke(_this9.outlineRed, _this9.outlineGreen, _this9.outlineBlue);
                    }
                  } else {
                    p.noStroke();
                  }

                  if (_this9.selectedBrush == 'ellipse') {
                    p.ellipse(p.mouseX, p.mouseY, _this9.selectedSize, _this9.selectedSize);
                  } else if (_this9.selectedBrush == 'rectangle') {
                    p.rect(p.mouseX, p.mouseY, _this9.selectedSize, _this9.selectedSize);
                  } else if (_this9.selectedBrush == 'triangle') {
                    p.triangle(p.mouseX, p.mouseY, p.mouseX + _this9.selectedSize / 2, p.mouseY - _this9.selectedSize, p.mouseX + _this9.selectedSize, p.mouseY);
                  } else if (_this9.selectedBrush == 'diamond') {
                    p.quad(p.mouseX, p.mouseY, p.mouseX + _this9.selectedSize / 2, p.mouseY - _this9.selectedSize, p.mouseX + _this9.selectedSize, p.mouseY, p.mouseX + _this9.selectedSize / 2, p.mouseY + _this9.selectedSize);
                  } //emit mouse data to socket.io server
                  //console.log(p.get().pixels);


                  _this9.webSocketService.emit('mouse', retData);

                  _this9.p = p;
                }; //socket.io event listeners
                //listen for an event from the socket.io server i.e: another user draws on the canvas


                _this9.webSocketService.listen('mouse').subscribe(function (data) {
                  //do something with the data sent from the socket.io server
                  if (data.showOutline == true) {
                    p.strokeWeight(data.outlineWidth);

                    if (data.eraser) {
                      p.stroke(data.backgroundR, data.backgroundG, data.backgroundB);
                    } else {
                      p.stroke(data.outlineRed, data.outlineGreen, data.outlineBlue);
                    }
                  } else {
                    p.noStroke();
                  }

                  if (data.fillShape) {
                    if (data.eraser) {
                      p.fill(data.backgroundR, data.backgroundG, data.backgroundB);
                    } else {
                      p.fill(data.r, data.g, data.b);
                    }
                  } else {
                    p.noFill();
                  }

                  if (data.brush == 'ellipse') {
                    p.ellipse(data.x, data.y, data.brushSize, data.brushSize);
                  } else if (data.brush == 'rectangle') {
                    p.rect(data.x, data.y, data.brushSize, data.brushSize);
                  } else if (data.brush == 'triangle') {
                    p.triangle(data.x, data.y, data.x + data.brushSize / 2, data.y - data.brushSize, data.x + data.brushSize, data.y);
                  } else if (data.brush == 'diamond') {
                    p.quad(data.x, data.y, data.x + data.brushSize / 2, data.y - data.brushSize, data.x + data.brushSize, data.y, data.x + data.brushSize / 2, data.y + data.brushSize);
                  }
                });

                _this9.webSocketService.listen('refresh').subscribe(function (data) {
                  //do something with the data sent from the socket.io server
                  if (data.refresh) {
                    p.resizeCanvas(data.canvasWidth, data.canvasHeight); //canvas = p.createCanvas(data.canvasWidth, data.canvasHeight) //don't call createCanvas multiple times

                    p.background(data.backgroundR, data.backgroundG, data.backgroundB);
                    _this9.backgroundR = data.backgroundR;
                    _this9.backgroundG = data.backgroundG;
                    _this9.backgroundB = data.backgroundB;
                    _this9.canvas.dimensions.height = data.canvasHeight;
                    _this9.canvas.dimensions.width = data.canvasWidth;
                  }

                  _this9.p = p;
                });
              };

              var player = new p5__WEBPACK_IMPORTED_MODULE_13__(s);
            });
          } catch (err) {
            console.error(err);
          }
        }
      }]);

      return CanvasComponent;
    }();

