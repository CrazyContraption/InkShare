    function () {
      function AppComponent(authService, jwtHelper) {
        var _this4 = this;

        _classCallCheck(this, AppComponent);

        this.authService = authService;
        this.jwtHelper = jwtHelper;
        this.animatePage = true;
        this.title = 'Inkshare';
        var token = this.authService.getToken();

        try {
          if (token) {
            this.account = this.jwtHelper.decodeToken(token);
            this.authService.reqresAccountGetById(this.account._id).subscribe(function (data) {
              _this4.account = data;
            });
          }
        } catch (err) {
          localStorage.clear();
          console.log(err);
        }
      }

      _createClass(AppComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
          if (this.account) this.tempTheme = this.account.theme.replace(/\s+/g, '-').toLowerCase();else this.tempTheme = "dark-pink";
          this.tempTheme += "-theme";
          document.getElementById('theme').classList.add(this.tempTheme);
        }
      }, {
        key: "getAnimationData",
        value: function getAnimationData(outlet) {
          return outlet && outlet.activatedRouteData && outlet.activatedRouteData['animation'];
        }
      }]);

      return AppComponent;
    }();

