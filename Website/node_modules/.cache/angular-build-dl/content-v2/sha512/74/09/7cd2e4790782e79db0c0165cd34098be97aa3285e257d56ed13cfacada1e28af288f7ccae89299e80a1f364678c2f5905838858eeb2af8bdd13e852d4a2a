let InterceptTokenService = class InterceptTokenService {
    // Initialization
    constructor(a) {
        this.a = a;
    }
    // Methods
    intercept(request, next) {
        request = request.clone({
            setHeaders: {
                Authorization: `JWT ${this.a.getToken()}`
            }
        });
        return next.handle(request);
    }
};
