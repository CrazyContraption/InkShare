            emailFormCtrl: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_6__["Validators"].required, _angular_forms__WEBPACK_IMPORTED_MODULE_6__["Validators"].pattern("^[A-z0-9._%+-]+@[A-z0-9.-]+\.[A-z]{2,}$")]]
        });
